{
  // Use IntelliSense to learn about possible attributes.
  // Hover to view descriptions of existing attributes.
  // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Flutter Profile",
      "request": "launch",
      "type": "dart",
      "flutterMode" : "profile"
    },
    {
      "name": "Flutter Debug CanvasKit",
      "request": "launch",
      "type": "dart",
      "flutterMode" : "debug",
      "program" : "lib/main.dart",
      "args": [
        "--web-renderer", "canvaskit"
      ],
    },
    {
      "name": "Flutter Release CanvasKit",
      "request": "launch",
      "type": "dart",
      "flutterMode" : "release",
      "program" : "lib/main.dart",
      "args": [
        "--web-renderer", "canvaskit"
      ],
    },
    {
      "name": "Flutter Release Html",
      "request": "launch",
      "type": "dart",
      "flutterMode" : "release",
      "program" : "lib/main.dart",
      "args": [
        "--web-renderer", "html"
      ],
    }
  ]
}